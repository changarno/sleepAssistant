<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sleepbgrest"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.mio.sleepassistant.MenuActivity"
    tools:showIn="@layout/app_bar_menu">

    <ImageButton
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/sleepsettings"
        android:id="@+id/SettingsButton"
        style="@android:style/Widget.Holo.Light.ImageButton"
        android:cropToPadding="false"
        android:background="@drawable/sleepsettings"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="14dp" />

    <ImageButton
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/sleepfave"
        android:layout_toEndOf="@+id/profileButton"
        android:layout_marginStart="20dp"
        android:id="@+id/imageButton4"
        android:background="@drawable/sleepfave"
        android:layout_alignParentBottom="true" />

    <ImageButton
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/sleepprofile"
        android:layout_marginStart="18dp"
        android:id="@+id/profileButton"
        android:background="@drawable/sleepprofile"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/SettingsButton" />


</RelativeLayout>
